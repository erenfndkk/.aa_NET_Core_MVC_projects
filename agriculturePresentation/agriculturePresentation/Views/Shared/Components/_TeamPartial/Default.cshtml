@using EntityLayer.Concreate
@model List<Team>

<div class="team" id="team">
		<div class="container">
			<h3 class="tittle">Ekip arkadaşlarımız</h3>
			<p class="heading-btm-w3ls">Birbirinden farklı alanlarda donanımlı ekip arkadaşlarımız.</p>
			<div class="wthree_team_grids">
				@foreach (var item in Model)
				{
					<div class="col-md-3 wthree_team_grid">
						<div class="hovereffect">
							<img src="@item.ImageUrl" alt=" " class="img-responsive" />
							<div class="overlay">
								<h6>Lorem ipsum</h6>
								<div class="rotate">
									<p class="group1">
										<a href="#"><i class="@item.TwitterUrl"></i></a>
										<a href="#"><i class="@item.FacebookUrl"></i></a>
									</p>
										<hr>
										<hr>
									<p class="group2">
										<a href="#"><i class="@item.InstagramUrl"></i></a>
										<a href="#"><i class="@item.WebsiteUrl"></i></a>
									</p>
								</div>
							</div>
						</div>
						<h4>@item.PersonName</h4>
						<p>@item.Title</p>
					</div>
				}
				<div class="clearfix"> </div>
			</div>
		</div>
	</div>